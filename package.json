{
  "name": "adcirc-lib",
  "version": "0.0.1",
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/atdyer/adcirc-lib",
  "main": "build/adcirc-lib.js",
  "author": {
    "name": "Tristan Dyer",
    "url": "https://github.com/atdyer"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/atdyer/adcirc-lib.git"
  },
  "scripts": {
    "dev": "rollup -w --banner \"$(preamble)\" -f umd -n adcirc -o build/adcirc-lib.js -- index.js",
    "minify": "uglifyjs --preamble \"$(preamble)\" build/adcirc-lib.js -c negate_iife=false -m -r build_fort14_worker,build_fortnd_worker -o build/adcirc-lib.min.js",
    "prepublish": "rm -rf build && mkdir build && rollup --banner \"$(preamble)\" -f umd -n adcirc -o build/adcirc-lib.js -- index.js"
  },
  "devDependencies": {
    "package-preamble": "0.0.2",
    "rollup": "^0.41.4",
    "rollup-watch": "^3.2.2",
    "uglify-js": "^2.8.8"
  }
}
